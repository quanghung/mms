<h1 class="offset1 pull-left">Edit Positon</h1>

<div class="row">
    <div class="span6 offset1">
      <%= form_for [:admin, @position], method: :patch do |f| %>
      <%= render "shared/flash_messages" %>
      <%= f.label :name %>
      <%= f.text_field :name %>
        
      <%= f.label :'Short Name' %>
      <%= f.text_field :short_name %>

      <%= f.submit "Edit Positon",class: "btn btn-large btn-primary" %>
      <% end %>
    </div>
</div>