<h1 class="offset1 pull-left">Add Team</h1>

<div class="row">
    <div class="span6 offset1">
      <%= form_for [:admin, @team] do |f| %>
      <%= render "shared/flash_messages" %>
      <%= f.label :name %>
      <%= f.text_field :name %>
        
      <%= f.label :'Description' %>
      <%= f.text_field :descriptions %>

      <%= f.submit "Add Team",class: "btn btn-large btn-primary" %>
      <% end %>
    </div>
</div>