<h1>All Projects</h1>
<%= render "shared/flash_messages" %>
<table border="1">
  <tr>
    <th>STT</th>
    <th>Project name</th>
    <th>Short name</th>
    <th>Start date</th>
    <th>End date</th>
    <th>Team id</th>
    <th>Team name</th>
  </tr>
  <% @projects.each do |project| %>
        <tr>
          <td><%= project.id %></td>
          <td><%= project.name %></td>
          <td><%= project.short_name %></td>
          <td><%= project.start_date %></td>
          <td><%= project.end_date %></td>
          <td><%= project.team_id %></td>
          <td><%= project.team.name %></td>
        </tr>
  <% end %>
</table>
<%= will_paginate %>



