<h1><%= @user.name %></h1>
<h2><%= link_to "Edit profile user", edit_admin_user_path,
    class: "btn btn-medium btn-primary" %></h2>

<div>
  Name: <%= @user.name %><br/>
  Email: <%= @user.email %><br/>
  Birthday: <%= @user.birthday %><br/>
  Team: <%= @user.team_id %>
</div>

<h4>Projects: <h4>
<ul>
  <% @user.projects.each do |project| %>
    <li>
      <%= project.name %>
    </li>
  <% end %>
</ul>

<h4>Position: </h4>
<ul>
	<li><%= @user.position.name %></li>
</ul>

<h4>
	Skills: 
</h4>
<ul>
  <% @user.skills.each do |skill| %>
    <li>
      <%= skill.name %>
    </li>
  <% end %>
</ul>